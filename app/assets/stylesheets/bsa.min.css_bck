    text-decoration: none;
    width: 100%;
}
a.smalllink:hover {
    color: #000000;
}
a.tooltip span {
    display: none;
    padding: 15px;
    width: 50%;
}
a.tooltip:hover span {
    background-color: #666666;
    background-image: -moz-linear-gradient(center top , #666666, #333333);
    border: 1px solid #111111;
    border-radius: 3px;
    color: #FFFFFF;
    display: block;
    font-size: 14px;
    line-height: 160%;
    margin: -125px 0 0 16%;
    position: absolute;
    text-align: center;
    text-shadow: 1px 1px #222222;
}
.qq-uploader {
    position: relative;
    width: 100%;
}
.qq-upload-button {
    background: none repeat scroll 0 0 #F6F6F6;
    border: 1px solid #E1E1E1;
    border-radius: 3px;
    box-shadow: -1px -1px 0 #FFFFFF inset;
    margin: 0;
    padding: 25px 20px;
}
.qq-upload-button-hover {
    background: none repeat scroll 0 0 #EEEEEE;
    border: 1px solid #CCCCCC;
}
.qq-upload-button-focus {
    outline: 1px dotted #000000;
}
.qq-upload-drop-area, .qq-upload-extra-drop-area {
    background: none repeat scroll 0 0 #F6F6F6;
    border: 1px solid #E1E1E1;
    border-radius: 3px;
    box-shadow: -1px -1px 0 #FFFFFF inset;
    display: block;
    height: 37px;
    left: 0;
    padding: 25px 20px;
    position: absolute;
    text-align: left;
    top: 0;
    width: 95.875%;
    z-index: 2;
}
.qq-upload-drop-area-active {
    background: none repeat scroll 0 0 #EEEEEE;
    border: 1px solid #CCCCCC;
    border-radius: 3px;
    box-shadow: -1px -1px 0 #FFFFFF inset;
    display: block;
    height: 37px;
    left: 0;
    padding: 25px 20px;
    position: absolute;
    text-align: left;
    top: 0;
    width: 95.875%;
    z-index: 2;
}
.qq-upload-finished {
    display: none;
    height: 15px;
    vertical-align: text-bottom;
    width: 15px;
}
.qq-upload-size, .qq-upload-cancel {
    font-size: 12px;
    font-weight: normal;
}
.qq-upload-failed-text {
    display: none;
    font-style: italic;
    font-weight: bold;
}
.qq-upload-failed-icon {
    display: none;
    height: 15px;
    vertical-align: text-bottom;
    width: 15px;
}
.qq-upload-fail .qq-upload-failed-text {
    display: inline;
}
.qq-upload-list li.qq-upload-success {
    background-color: #5DA30C;
    color: #FFFFFF;
}
.qq-upload-list li.qq-upload-fail {
    background-color: #D60000;
    color: #FFFFFF;
}
